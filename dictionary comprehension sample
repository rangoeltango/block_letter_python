#Creating two sample lists to include song names and the number of plays
songs = ["Ordun Delerei", "Lord of Lightning", "Billabong Valley", "Gondii"]
playcounts = [728, 239, 444, 241, 839, 522]

plays = {songs:playcounts for songs, playcounts in zip(songs, playcounts)}
print(plays)

plays["Space Oddity"] = 1
plays["Gondii"] = 540

library = {"The Best Songs": plays, "Sunday Feelings": {}}
print(library)
